<?xml version="1.0" encoding="utf-8" ?>
<tests>
	<variables>
    <variable name="LOGIN_URL" environment="DevTeam1">http://myserver/login.php</variable>
    <variable name="LOGIN_URL" environment="DevTeam2">http://myserver2/login.php</variable>
		<variable name="LOGIN1" environment="DevTeam2">bob</variable>
		<variable name="PASSWD1" environment="DevTeam1">sponge</variable>
		<variable name="SUCCESSFULL_TEST_TEXT" environment="DevTeam2">Welcome Bob</variable>
	</variables>

	<test
		id="1"
		description="description 1"
		method="post"
		url="http://myserver/test/login.jsp"
	>
		<postbody>
			<![CDATA[username=corey&password=welcome]]>
		</postbody>

		<assertions>
			<verify type="positive" description="1" method="regex">verify this string exists</verify>
			<verify type="negative" description="1" method="regex">verify this string does not exist</verify>
		</assertions>
		
		<capturedvariables>
			<variable name="test">/d</variable>
		</capturedvariables>
	</test>

	<test
		id="300"
