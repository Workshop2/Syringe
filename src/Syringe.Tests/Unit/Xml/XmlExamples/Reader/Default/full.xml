<?xml version="1.0" encoding="utf-8" ?>
<testcases repeat="10">
    <variables>
        <variable name="LOGIN_URL">http://myserver/login.php</variable>
        <variable name="LOGIN1">bob</variable>
        <variable name="PASSWD1">sponge</variable>
        <variable name="SUCCESSFULL_TEST_TEXT">Welcome Bob</variable>
    </variables>

    <case
		id="1"
		shortdescription="short description 1"
		longdescription="long description"
		method="post"
		url="http://myserver/test/login.jsp"
		logrequest="true"
		logresponse="true"
		sleep="3"
	>
        <postbody>
            <![CDATA[username=corey&password=welcome]]>
        </postbody>
        
        <verifications>
            <verify type="positive" description="1">verify this string exists</verify>
			<verify type="negative" description="1">verify this string does not exist</verify>
		</verifications>
      <parseresponses>
        <parseresponse description="test">/d</parseresponse>
      </parseresponses>
    </case>

    <case
		id="300"
		shortdescription="short description 300"
		longdescription="long description"
		method="get"
		url="http://myserver/test/send.jsp?value={TIMESTAMP}"
	>
		<verifications>
			<verify type="positive" id="1">verify this string exists</verify>
        </verifications>
    </case>

    <case
		id="20"
		shortdescription="short description 20"
		longdescription="long description"
		method="get"
		url="http://myserver/test/send.jsp?value={TIMESTAMP}"
	>
		<verifications>
			<verify type="positive" description="1">verify this string exists</verify>
        </verifications>
    </case>

    <case
