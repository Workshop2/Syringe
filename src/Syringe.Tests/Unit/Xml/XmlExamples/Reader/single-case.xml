<?xml version="1.0" encoding="utf-8" ?>
<tests repeat="10">
    <test
        id="1"
        shortdescription="short description"
        longdescription="long description"
        url="http://myserver"
        method="post"
        posttype="text/xml"
        verifyresponsecode="404"
        errormessage="my error message"
        logrequest="false"
        logresponse="false"
	    >

        <headers>
            <header name="mykey">12345</header>
            <header name="bar">foo</header>
            <header name="emptyvalue"></header>
            <header name="Cookie">referer=harrispilton.com</header>
        </headers>

        <postbody>
            <![CDATA[
            username=corey&password=welcome
            ]]>
        </postbody>

        <capturedvariables>
            <variable description="1">parse 1</variable>
            <variable description="2">parse 11</variable>
            <variable description="3">parse 99</variable>
        </capturedvariables>

		<assertions>
			<assert type="positive" description="1">positive 1</assert>
			<assert type="PosItiVe" description="2">positive 22</assert>
			<assert type="positive" description="3">positive 99</assert>

			<assert type="negative" description="1">negative 1</assert>
			<assert type="NEGaTiVE" description="2">negative 6</assert>
			<assert type="negative" description="3">negative 66</assert>
        </assertions>
    </test>
</tests>