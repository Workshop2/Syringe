@model List<CapturedVariableItem>

@if (Model.Count > 0)
{
    <div class="alert alert-info">
        <strong>Available variables</strong>

        <ul>
            @foreach (var item in Model)
            {
                <li>
                    {@item.Description} - @item.Regex
                </li>
            }
        </ul>
    </div>
}