@model Syringe.Core.Tests.TestFileOrder

<h2>Reorder tests for @Model.Filename</h2>
<ul  class="list-group" id="reorderedTestsList">
    @foreach (var item in Model.Tests)
    {
        <li class="list-group-item" data-original="@item.OriginalPostion"><span class="drag-handle">&#9776;</span>@item.Description</li>
    }

</ul>

<button id="saveOrder" data-filename="@Model.Filename">Save</button>
<button id="cancelOrder" data-dismiss="modal">Cancel</button>
